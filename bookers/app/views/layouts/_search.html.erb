<%= form_with url: "/search", method: :get, local: true do |f| %>
  <%= f.text_field 'search[content]' %>
  <%= f.select 'search[model]',options_for_select({ "User" => "user", "Book" => "book" }) %> <!--Userの文字はuserとして情報を送ります-->
  <%= f.select 'search[method]', "完全一致" => "perfect_match", "前方一致" => 'forward_match', "後方一致" => "backward_mathch","部分一致"=>"partial" %> <!--完全一致の文字はperfect_matchとして情報を送ります-->
  <%= f.submit '検索' %>
<% end %>